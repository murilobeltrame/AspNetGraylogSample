{
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Error"
        },
        "Console": {
            "LogLevel": {
                "Default": "Debug"
            }
        },
        "GELF": {
            "IncludeScopes": true,
            "LogLevel": {
                "Default": "Information"
            }
        }
    },
    "Graylog": {
        "Host": "localhost",
        "Port": 12201,
        "LogSource": "application-name"
    },
    "AllowedHosts": "*"
}